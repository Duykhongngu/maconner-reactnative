{
  "expo": {
    "name": "Duy dang ngu",
    "slug": "maconer-reactnative",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/imageapp.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/imageapp.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to let you upload review images.",
          "cameraPermission": "The app needs access to your camera to let you take review photos."
        }
      ],
      "expo-router",
      "expo-font"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.duynguroi1504.marconerreacnative",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/imageapp.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.duynguroi1504.marconerreacnative",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/imageapp.jpg"
    },
    "experiments": {
      "typedRoutes": true
    },
    "packagerOpts": {
      "config": "metro.config.js"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "344399a1-a42e-47e7-84a8-a7936cdc467a"
      }
    },
    "owner": "duynguroi1504"
  }
}
